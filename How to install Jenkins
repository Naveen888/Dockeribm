248  apt install maven
  249  wget -q -O - https://pkg.jenkins.io/debian-stable/jenkins.io.key | sudo apt-key add -
  250  sudo sh -c 'echo deb https://pkg.jenkins.io/debian-stable binary/ > /etc/apt/sources.list.d/jenkins.list'
  251  sudo apt-get update
  252  sudo apt-get install jenkins
  253  systemctl status jenkins
